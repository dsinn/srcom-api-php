{
    "name": "dsinn/srcom-api-php",
    "description": "An API client for speedrun.com, written in PHP.",
    "keywords": ["speedrun.com"],
    "license": "MIT",
    "type": "library",
    "require": {
        "ext-json": "*",
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dsinn\\SrcomApi\\": "src/",
            "Dsinn\\SrcomApi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "suggest": {
        "kevinrob/guzzle-cache-middleware": "To cache the responses."
    }
}
